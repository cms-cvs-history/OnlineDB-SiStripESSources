module PopulateDb = SiStripPopulateConfigDb {
        untracked   int32 MaxNumberOfDets  = 0
        untracked   int32 ChannelsPerFed   = 84
	untracked    bool UsingDb          = false
	untracked  string InputModuleXml   = ""
	untracked  string InputDcuInfoXml  = ""
	untracked vstring InputFecXml      = { "" }
	untracked vstring InputFedXml      = { "" }
	untracked  string OutputModuleXml  = "/tmp/module.xml"
	untracked  string OutputDcuInfoXml = "/tmp/dcuinfo.xml"
	untracked  string OutputFecXml     = "/tmp/fec.xml"
	untracked  string OutputFedXml     = "/tmp/fed.xml"
}

// untracked bool UsingDb:		[default=true]			switch between using configuration database or xml files
// untracked string InputModuleXml:	[default=""]			input xml file containing HW connection info (used if UsingDb=false)
// untracked string InputDcuInfoXml:	[default=""]			input xml file containing DCU - DetId map (used if UsingDb=false)
// untracked vstring InputFecXml:	[default=""]			input xml file(s) containing control info (used if UsingDb=false)
// untracked vstring InputFedXml:	[default=""]			input xml file(s) containing readout info (used if UsingDb=false)
// untracked string OutputModuleXml:	[default="/tmp/module.xml"]	output xml file containing HW connection info (used if UsingDb=false)
// untracked string OutputDcuInfoXml:	[default="/tmp/dcuinfo.xml"]	output xml file containing DCU - DetId map (used if UsingDb=false)
// untracked string OutputFecXml:	[default="/tmp/fec.xml"]	output xml file containing control info (used if UsingDb=false)
// untracked string OutputFedXml:	[default="/tmp/fed.xml"]	output xml file containing readout info (used if UsingDb=false)

